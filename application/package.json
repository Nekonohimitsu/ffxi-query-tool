{
  "name": "ffxi-query-tool",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf ./output",
    "start": "webpack-dev-server --progress --colors --port 8082 --host 0.0.0.0 --hot --inline --content-base .",
    "compile": "yarn install && yarn clean ** mkdirp ./output && webpack --config webpack.config.js",
    "lint": "tsc --noEmit && eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "prettier:write": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json}"
  },
  "author": "Neko",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nekonohimitsu/ffxi-query-tool.git"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
